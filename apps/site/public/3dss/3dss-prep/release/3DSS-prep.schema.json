{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://q2t-project.github.io/3dsl/schemas/3DSS-prep.schema.json#v1.0.0",
  "title": "3DSS Prep Schema v1.0.0",
  "type": "object",
  "properties": {
    "points": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" }
        },
        "required": ["name"]
      }
    },
    "document_meta": {
      "type": "object",
      "properties": {
        "schema_uri": { "type": "string", "format": "uri" },
        "generator": { "type": "string" },
        "source": { "type": "string" }
      },
      "required": ["schema_uri"],
      "additionalProperties": true
    }
  },
  "required": ["points"],
  "additionalProperties": true
}
